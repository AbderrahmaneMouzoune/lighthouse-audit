{
  "compilerOptions": {
      "module": "ESNext",
      "target": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "lib": ["ESNext", "DOM"],
      "outDir": "./build",
      "strict": true
  },
  "include": ["src/**/*.ts", "src/*.ts"],
  "exclude": ["node_modules"]
}